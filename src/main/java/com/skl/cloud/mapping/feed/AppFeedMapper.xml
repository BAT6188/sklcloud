	<!-- 根据指定userId查询所在的家庭组id -->
	<select id="queryAllCircleIdsByUserId" parameterType="long"
		resultType="long">
	<![CDATA[
	    SELECT DISTINCT circle_id  circleId from t_app_circle_member where member_id = #{0}
	]]>
	</select>

	<!-- 根据指定circleId查询所有的userId -->
	<select id="queryAllUserIdsByCircleId" parameterType="long"
		resultType="long">
	<![CDATA[
	    SELECT member_id memberId FROM t_app_circle_member where circle_id = #{0}
	]]>
	</select>

	<!-- 根据指定userId查询对应的分享出来的sn -->
	<select id="queryAllSnByUserId" parameterType="java.util.Map" resultType="String">
	<![CDATA[
	    SELECT 
	    DISTINCT
			ca.camera_serialno
		from 
			t_app_share_device d,
			t_app_user_camera ca
 		where circle_id IN(
				SELECT circle_id from t_app_circle_member where member_id = #{userId})
			AND
				d.member_id = #{userId}
			AND
				d.device_status = 0
			AND
				ca.camera_id = d.device_id
            AND circle_id = #{circleId}
	]]>
	</select>
