package com.skl.cloud.model.ipc;

import java.util.Date;

import com.skl.cloud.common.entity.IdEntity;

@SuppressWarnings("serial")
public class IPCameraSub extends IdEntity {

	// 设备sn
	private String sn;
	
	// 摄像头验证码
	private String pincode;
	
	// 摄像头认证日期
	private Date validate;

	// 摄像头失效日期
	private Date expiredDate;
	
	// 摄像头软版本号
	private String version;

	// 软件版本日期
	private Date versionDate;
	
	// fw升级(fw下载状态)
	private Integer fwDownloadStatus;

	// fw升级原因说明(fw下载状态)
	private String fwDownloadErrorReason;
	
	// 摄像头随机数
	private String random;

	// 摄像头随机数, 收到IPC的
	private String receIpcRandom;

	// 云端随机数, 发送给IPC的
	private String sendIpcRandom;

	// 云端P2P随机数, app读取p2p信息时获得
	private String p2pRandom;

	// 视频的分享时长(秒)(wechat+)
	private Long videoShareExpire;

	/**
	 * getter method
	 * @return the sn
	 */
	public String getSn() {
		return sn;
	}

	/**
	 * setter method
	 * @param sn the sn to set
	 */
	public void setSn(String sn) {
		this.sn = sn;
	}

	/**
	 * getter method
	 * @return the pincode
	 */
	public String getPincode() {
		return pincode;
	}

	/**
	 * setter method
	 * @param pincode the pincode to set
	 */
	public void setPincode(String pincode) {
		this.pincode = pincode;
	}

	/**
	 * getter method
	 * @return the validate
	 */
	public Date getValidate() {
		return validate;
	}

	/**
	 * setter method
	 * @param validate the validate to set
	 */
	public void setValidate(Date validate) {
		this.validate = validate;
	}

	/**
	 * getter method
	 * @return the expiredDate
	 */
	public Date getExpiredDate() {
		return expiredDate;
	}

	/**
	 * setter method
	 * @param expiredDate the expiredDate to set
	 */
	public void setExpiredDate(Date expiredDate) {
		this.expiredDate = expiredDate;
	}

	/**
	 * getter method
	 * @return the version
	 */
	public String getVersion() {
		return version;
	}

	/**
	 * setter method
	 * @param version the version to set
	 */
	public void setVersion(String version) {
		this.version = version;
	}

	/**
	 * getter method
	 * @return the versionDate
	 */
	public Date getVersionDate() {
		return versionDate;
	}

	/**
	 * setter method
	 * @param versionDate the versionDate to set
	 */
	public void setVersionDate(Date versionDate) {
		this.versionDate = versionDate;
	}

	/**
	 * getter method
	 * @return the fwDownloadStatus
	 */
	public Integer getFwDownloadStatus() {
		return fwDownloadStatus;
	}

	/**
	 * setter method
	 * @param fwDownloadStatus the fwDownloadStatus to set
	 */
	public void setFwDownloadStatus(Integer fwDownloadStatus) {
		this.fwDownloadStatus = fwDownloadStatus;
	}

	/**
	 * getter method
	 * @return the fwDownloadErrorReason
	 */
	public String getFwDownloadErrorReason() {
		return fwDownloadErrorReason;
	}

	/**
	 * setter method
	 * @param fwDownloadErrorReason the fwDownloadErrorReason to set
	 */
	public void setFwDownloadErrorReason(String fwDownloadErrorReason) {
		this.fwDownloadErrorReason = fwDownloadErrorReason;
	}

	/**
	 * getter method
	 * @return the random
	 */
	public String getRandom() {
		return random;
	}

	/**
	 * setter method
	 * @param random the random to set
	 */
	public void setRandom(String random) {
		this.random = random;
	}

	/**
	 * getter method
	 * @return the receIpcRandom
	 */
	public String getReceIpcRandom() {
		return receIpcRandom;
	}

	/**
	 * setter method
	 * @param receIpcRandom the receIpcRandom to set
	 */
	public void setReceIpcRandom(String receIpcRandom) {
		this.receIpcRandom = receIpcRandom;
	}

	/**
	 * getter method
	 * @return the sendIpcRandom
	 */
	public String getSendIpcRandom() {
		return sendIpcRandom;
	}

	/**
	 * setter method
	 * @param sendIpcRandom the sendIpcRandom to set
	 */
	public void setSendIpcRandom(String sendIpcRandom) {
		this.sendIpcRandom = sendIpcRandom;
	}

	/**
	 * getter method
	 * @return the p2pRandom
	 */
	public String getP2pRandom() {
		return p2pRandom;
	}

	/**
	 * setter method
	 * @param p2pRandom the p2pRandom to set
	 */
	public void setP2pRandom(String p2pRandom) {
		this.p2pRandom = p2pRandom;
	}

	/**
	 * getter method
	 * @return the videoShareExpire
	 */
	public Long getVideoShareExpire() {
		return videoShareExpire;
	}

	/**
	 * setter method
	 * @param videoShareExpire the videoShareExpire to set
	 */
	public void setVideoShareExpire(Long videoShareExpire) {
		this.videoShareExpire = videoShareExpire;
	}
}