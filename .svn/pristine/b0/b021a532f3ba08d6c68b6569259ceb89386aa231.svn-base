package com.skl.cloud.model;

import java.io.Serializable;
import java.math.BigDecimal;

import com.skl.cloud.model.user.AppUser;

/**
 * @Package com.skl.cloud.model
 * @Title: PlatformConfig
 * @Description: t_platform_log实体类
