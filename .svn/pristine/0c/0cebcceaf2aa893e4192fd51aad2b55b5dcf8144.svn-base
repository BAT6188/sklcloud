package com.skl.cloud.model;

import java.io.Serializable;
import java.util.Date;

/**
 * @ClassName: AppDeviceShare
 * @Description: 家庭组（圈子）管理-设备分享对象
 * @author zhaonao
 * @date 2015年10月12日
 *
 */
public class AppDeviceShare implements Serializable
{
	/**
	 * 序列化
	 */
	private static final long serialVersionUID = -5669174761218197094L;

	/**
	 * ID（主键）
	 */
	private String id;

	/**
	 * 家庭组ID
	 */
	private String circleId;

	/**
	 * 家庭组成员ID
	 */
	private String memberId;

	/**
	 * 设备ID（分享）
	 */
	private String deviceId;

	/**
	 * 设备权限使能-视频流
	 */
	private String streamEnable;

	/**
	 * 设备权限使能-视频回放
	 */
	private String playbackEnable;

	/**
	 * 设备权限使能-设备通知
	 */
	private String notificationEnable;

	/**
	 * 是否过滤掉（不查看）某个用户所能看到的某个设备的feeds内容
	 */
	private String feedsFilterEnable;

	/**
	 * 家庭组中的设备状态(0：绑定;1：分享;2：取消分享)
	 */
	private String deviceStatus;

	/**
	 * 创建时间
	 */
	private Date createTime;

	/**
	 * 最后修改时间
	 */
	private Date changeTime;

	public String getId()
	{
		return id;
	}

	public void setId(String id)
	{
		this.id = id;
	}

	public String getCircleId()
	{
		return circleId;
	}

	public void setCircleId(String circleId)
	{
		this.circleId = circleId;
	}

	public String getMemberId()
	{
		return memberId;
	}

	public void setMemberId(String memberId)
	{
		this.memberId = memberId;
	}

	public String getDeviceId()
	{
		return deviceId;
	}

	public void setDeviceId(String deviceId)
	{
		this.deviceId = deviceId;
	}

	public String getStreamEnable()
	{
		return streamEnable;
	}

	public void setStreamEnable(String streamEnable)
	{
		this.streamEnable = streamEnable;
	}

	public String getPlaybackEnable()
	{
		return playbackEnable;
	}

	public void setPlaybackEnable(String playbackEnable)
	{
		this.playbackEnable = playbackEnable;
	}

	public String getNotificationEnable()
	{
		return notificationEnable;
	}

	public void setNotificationEnable(String notificationEnable)
	{
		this.notificationEnable = notificationEnable;
	}

	public String getFeedsFilterEnable()
	{
		return feedsFilterEnable;
	}

	public void setFeedsFilterEnable(String feedsFilterEnable)
	{
		this.feedsFilterEnable = feedsFilterEnable;
	}

	public String getDeviceStatus()
	{
		return deviceStatus;
	}

	public void setDeviceStatus(String deviceStatus)
	{
		this.deviceStatus = deviceStatus;
	}

	public Date getCreateTime()
	{
		return createTime;
	}

	public void setCreateTime(Date createTime)
	{
		this.createTime = createTime;
	}

	public Date getChangeTime()
	{
		return changeTime;
	}

	public void setChangeTime(Date changeTime)
	{
		this.changeTime = changeTime;
	}
}
